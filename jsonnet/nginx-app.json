{
   "apiVersion": "v1",
   "items": [
      {
         "apiVersion": "extensions/v1beta1",
         "kind": "Deployment",
         "metadata": {
            "name": "nginx-deployment"
         },
         "spec": {
            "replicas": 2,
            "template": {
               "metadata": {
                  "labels": {
                     "app": "nginx"
                  }
               },
               "spec": {
                  "containers": [
                     {
                        "image": "nginx:1.7.9",
                        "name": "nginx",
                        "ports": [
                           {
                              "containerPort": 80
                           }
                        ],
                        "volumeMounts": [
                           {
                              "mountPath": "/logs",
                              "name": "logging-vol",
                              "readOnly": false
                           }
                        ]
                     }
                  ],
                  "volumes": [
                     {
                        "emptyDir": { },
                        "name": "logging-vol"
                     }
                  ]
               }
            }
         }
      },
      {
         "apiVersion": "v1",
         "kind": "Service",
         "metadata": {
            "labels": {
               "app": "nginx"
            },
            "name": "my-nginx"
         },
         "spec": {
            "ports": [
               {
                  "port": 80,
                  "targetPort": 80
               }
            ],
            "selector": {
               "app": "nginx"
            },
            "type": "LoadBalancer"
         }
      }
   ],
   "kind": "List"
}
